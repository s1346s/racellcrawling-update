<?xml version="1.0" encoding="UTF-8"?>
<configuration
	xmlns="http://update4j.org/schema/update-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
                 http://update4j.org/schema/update-config
                 schema/update-config.xsd">

	<!-- 모든 파일이 받을 기본 URL -->
	<base>https://github.com/s1346s/racellcrawling-update/releases/download/v1.1/
	</base>

	<files>
		<!-- classpath에 올릴 JAR -->
		<file uri="automation-racellcrawling-deploy-1.1-SNAPSHOT.jar"
			path="automation-racellcrawling-deploy-1.1-SNAPSHOT.jar"
			algorithm="SHA-256"
			classpath="true">
		</file>
	</files>


	<launch>
		<file path="automation-racellcrawling-deploy-1.1-SNAPSHOT.jar" />
		<class>com.shj.racellcrawling.ServiceCrawlingApp</class>
	</launch>
</configuration>
