<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns="http://update4j.org/schema/update-config"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://update4j.org/schema/update-config
                                  schema/update-config.xsd">

    <!-- 1.5.9에서는 텍스트-내용 방식의 <base>를 사용할 수 있습니다 -->
    <base>https://github.com/s1346s/racellcrawling-update/releases/download/v1.1/</base>

    <files>
        <file
            uri="automation-racellcrawling-deploy-1.1-SNAPSHOT.jar"
            path="automation-racellcrawling-deploy-1.1-SNAPSHOT.jar"
            classpath="true"/>
    </files>

    <launch>
        <file path="automation-racellcrawling-deploy-1.1-SNAPSHOT.jar"/>
        <class>com.shj.racellcrawling.ServiceCrawlingApp</class>
    </launch>
</configuration>
